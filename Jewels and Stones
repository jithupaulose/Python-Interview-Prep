https://leetcode.com/problems/jewels-and-stones/

class Solution:
    def numJewelsInStones(self, jewels: str, stones: str) -> int:
        jewels_set = set(jewels)
        output_total = 0
        for i in stones:
            if i in jewels_set:
                output_total +=1
        return output_total
                
        
